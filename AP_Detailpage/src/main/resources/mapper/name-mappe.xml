<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ap.consumer.dao.nameDAO">

<select id="getname2" resultType="com.ap.consumer.vo.NameVO">
	select * from AnimalPharmacy where REFINE_ROADNM_ADDR = #{REFINE_ROADNM_ADDR}
</select>

<select id="getname" resultType="com.ap.consumer.vo.NameVO">
	select * from AnimalPharmacy where BIZPLC_NM = #{BIZPLC_NM} and ROADNM_ZIP_CD = #{ROADNM_ZIP_CD}
</select>

</mapper>